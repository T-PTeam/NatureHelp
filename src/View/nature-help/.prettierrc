{
    "printWidth": 120,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "arrowParens": "always",
    "proseWrap": "preserve",
    "endOfLine": "lf",
    "singleAttributePerLine": true,
    "plugins": ["prettier-plugin-tailwindcss"],
    "formatOnSave": true
}
